{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": ["prettier"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "prettier/prettier": "error",
        "indent":"off",
        "object-curly-newline":"off",
        "radix":"off",
        "prefer-const":"off",
        "no-param-reassign": "off",
        "class-methods-use-this":"off",
        "no-undef": "off",
        "no-unused-vars":"off",
        "no-alert": "off",
        "no-eval":"off",
        "no-restricted-globals":"off",
        "guard-for-in":"off",
        "no-restricted-syntax":"off"
    }
}